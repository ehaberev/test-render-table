{{page-title "RawTable"}}

<div>
  <LinkTo @route="index">Назад</LinkTo>
</div>

<div>
  <button {{on "click" this.toggleStartLoading}}>
    {{#if this.startedLoading}}
      Остановить загрузку данных
    {{else}}
      Начать загрузку данных
    {{/if}}
  </button>
</div>

<table class="MuiTable-root jss222 jss203">
  <colgroup><col style="width: 128px;"><col><col><col><col style="width: 128px;"><col style="width: 256px;"><col></colgroup>
  <thead class="MuiTableHead-root">
    <tr class="MuiTableRow-root MuiTableRow-head">
      <th class="MuiTableCell-root MuiTableCell-head jss227 jss237">Received</th>
      <th class="MuiTableCell-root MuiTableCell-head jss227 jss237">Pick-up at</th>
      <th class="MuiTableCell-root MuiTableCell-head jss227 jss237">Deliver to</th>
      <th class="MuiTableCell-root MuiTableCell-head jss227 jss237">Vehicle</th>
      <th class="MuiTableCell-root MuiTableCell-head jss227 jss237">Miles</th>
      <th class="MuiTableCell-root MuiTableCell-head jss227 jss237">Match</th>
      <th class="MuiTableCell-root MuiTableCell-head jss227 jss237">Brokerage</th>
    </tr>
  </thead>
  <tbody class="MuiTableBody-root">
    {{#each this.rows as |item index|}}
      <tr class="{{if item.pinned "MuiTableRow-root jss688 jss689 jss690 jss691 jss692" (if (gt item.bids.length 0) "MuiTableRow-root jss688 jss695" "MuiTableRow-root jss688")}}" {{on "click" this.onClick}}>
        <td class="MuiTableCell-root MuiTableCell-body jss242 jss246">
          {{moment-format item.receivedDate "H:mm A"}}
        </td>
        <td class="MuiTableCell-root MuiTableCell-body jss242 jss246">
          {{item.pickUpAt}}
          {{#if (mod index 3)}}
            <svg class="MuiSvgIcon-root jss734 MuiSvgIcon-fontSizeSmall" focusable="false" viewBox="0 0 24 24" role="img"><path d="M13.5.67s.74 2.65.74 4.8c0 2.06-1.35 3.73-3.41 3.73-2.07 0-3.63-1.67-3.63-3.73l.03-.36C5.21 7.51 4 10.62 4 14c0 4.42 3.58 8 8 8s8-3.58 8-8C20 8.61 17.41 3.8 13.5.67zM11.71 19c-1.78 0-3.22-1.4-3.22-3.14 0-1.62 1.05-2.76 2.81-3.12 1.77-.36 3.6-1.21 4.62-2.58.39 1.29.59 2.65.59 4.04 0 2.65-2.15 4.8-4.8 4.8z"></path><title>ASAP</title></svg>
          {{/if}}
        </td>
        <td class="MuiTableCell-root MuiTableCell-body jss242 jss246">{{item.deliverTo}}</td>
        <td class="MuiTableCell-root MuiTableCell-body jss242 jss246">{{item.suggestedTruck}}</td>
        <td class="MuiTableCell-root MuiTableCell-body jss242 jss246">{{item.miles}}</td>
        <td class="MuiTableCell-root MuiTableCell-body jss242 jss246">
          <div><svg class="MuiSvgIcon-root MuiSvgIcon-fontSizeSmall" focusable="false" viewBox="0 0 24 24" role="img" style="vertical-align: middle;"><path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1.41 16.09V20h-2.67v-1.93c-1.71-.36-3.16-1.46-3.27-3.4h1.96c.1 1.05.82 1.87 2.65 1.87 1.96 0 2.4-.98 2.4-1.59 0-.83-.44-1.61-2.67-2.14-2.48-.6-4.18-1.62-4.18-3.67 0-1.72 1.39-2.84 3.11-3.21V4h2.67v1.95c1.86.45 2.79 1.86 2.85 3.39H14.3c-.05-1.11-.64-1.87-2.22-1.87-1.5 0-2.4.68-2.4 1.64 0 .84.65 1.39 2.67 1.91s4.18 1.39 4.18 3.91c-.01 1.83-1.38 2.83-3.12 3.16z"></path><title>583</title></svg>{{item.matchCount}} / 3<span>&nbsp;(<svg class="MuiSvgIcon-root MuiSvgIcon-fontSizeInherit" focusable="false" viewBox="0 0 24 24" aria-hidden="true" style="vertical-align: top;"><path d="M21 16v-2l-8-5V3.5c0-.83-.67-1.5-1.5-1.5S10 2.67 10 3.5V9l-8 5v2l8-2.5V19l-2 1.5V22l3.5-1 3.5 1v-1.5L13 19v-5.5l8 2.5z"></path></svg>{{item.matchCount}} mi)</span></div>
        </td>
        <td class="MuiTableCell-root MuiTableCell-body jss242 jss246">{{item.contactName}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

{{outlet}}